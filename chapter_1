        "  except:\n",
        "    print('Year inserted incorrectly.')\n",
        "    \n",
        "if (year % 400 == 0) or (year % 4 == 0 and year % 100 != 0): #checking if the condition is met\n",
        "  print('{} is a leap year.'.format(year)) #expression to be printed if the condition is met \n",
        "else: \n",
        "  print('{} is not a leap year.'.format(year)) #expression to be printed in any other case"
